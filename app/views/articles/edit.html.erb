<%= form_with model: @article do |form|%>
  <%= form.label :title%>
  <%= form.text_field :title, value: @article.title%>
  <%= form.label :content%>
  <%= form.text_area :content, value: @article.content%>
  <%#= form.select :category_id, Category.all.collect {|category| [category.name, category.id]}%>
  <%= form.submit "Update Article"%>
<% end %>